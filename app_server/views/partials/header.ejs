<!-- modal two -->
        <div id="signup" class="uk-flex-top auth-form" uk-modal>
            <div class="uk-modal-dialog uk-modal-body uk-margin-auto-vertical auth-wrapper">
        
                <button class="uk-modal-close-default" type="button" uk-close></button>
                <h4>Register Below</h4>

                <form action="" uk-grid>
                    <div class="uk-width-1-2@s">
                        <input class="uk-input" type="text" name="username" placeholder="Enter First Name">
                    </div>
                    <div class="uk-width-1-2@s">
                        <input class="uk-input" type="text" name="last" placeholder="Enter Last name">
                    </div>

                    <div class="uk-width-1-1">
                        <input class="uk-input" type="email" name="email" placeholder="Enter E-mail">
                    </div>

                    <div class="uk-width-1-1">
                        <input class="uk-input" type="text" placeholder="What's app">
                    </div>


                    <div class="uk-margin uk-width-1-1">
                        <div class="uk-form-controls">
                            <select class="uk-select" id="form-stacked-select">
                                <option>I want to</option>
                                <option>Short Let</option>
                                <option>Rent</option>
                                <option>Buy</option>
                                <option>Build</option>
                                <option>Design</option>
                                <option>Sell</option>
                                <option>Let</option>
                                <option>Invest</option>
                            </select>
                        </div>
                    </div>
                    <button class='myButt one signup-btn'>
                        <div class='insider'></div>
                        Register Now
                    </button>
                   
                </form>
        
            </div>
        </div>
    
<!-- modal three     -->
        <div id="signup-complete" class="uk-flex-top auth-form" uk-modal>
            <div class="uk-modal-dialog uk-modal-body uk-margin-auto-vertical init-pop-up">

                <button class="uk-modal-close-default" type="button" uk-close></button>
                <h5>Thank You</h5>
                <img src="/images/congrats.png" alt="" class="modal-img">
                <p>Email will be send to you within 48 hours with the webinar details.</p>
                


                <div class="btn-wrapper">
                    <a href="#" class="uk-modal-close-default myButt one" type="button">
                        <div class='insider'></div>
                        Close
                    </a>
                </div>
                
            </div>
    </div>
    
<!-- Log in -->
        <div id="login" class="uk-flex-top auth-form" uk-modal>
            <div class="uk-modal-dialog uk-modal-body uk-margin-auto-vertical auth-wrapper">
        
                <button class="uk-modal-close-default" type="button" uk-close></button>
                <h3><span>X</span>CELZS</h3>
                <h4>Hello again <br>Welcome back</h4>
                <form action="/xcelzs/login" method="POST">
                    <label for="">Enter username</label>
                    <input type="text" name="username" placeholder="Username here">
                    <label for="">Enter e-mail</label>
                    <input type="email" name="email" placeholder="E-mail here">
                    <label for="">Enter password</label>
                    <input type="password" name="password" placeholder="Passwoord here">
                    <button>LOG IN</button>
                </form>
        
            </div>
        </div>

       

